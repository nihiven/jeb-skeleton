{
	"name": "my-skeleton-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"prisma": "npx prisma studio",
		"prisma-migrate": "npx prisma migrate dev --name 'Migrate Me'",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma-push": "npx prisma db push",
		"prisma-generate": "npx prisma generate",
		"prisma-seed": "npx prisma db seed --preview-feature",
		"prisma-reset": "npx prisma db reset --preview-feature",
		"prisma-init": "npx prisma init --datasource-provider sqlite"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^0.92.2",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-netlify": "^1.0.4",
		"@sveltejs/kit": "^1.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.9.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"prisma": {
		"seed": "pnpm exec vite-node prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^4.9.0"
	}
}
